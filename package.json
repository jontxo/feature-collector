    "update-package-json-dependencies": "npx ncu -u",
    "generate:eslintrc": "npx tslint-to-eslint-config",
    "eslint-fix-all": "npx eslint lib/**/*.ts test/**/*.ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://yon_cuadrado@bitbucket.org/yon_cuadrado/cucumber-node-html-reporter.git"
  },
  "author": "Yon Cuadrado",
  "dependencies": {
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "fs-extra": "9.0.0",
    "json-future": "2.2.4"
  },
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/chai-as-promised": "7.1.2",
    "@types/fs-extra": "8.1.0",
    "@types/long": "4.0.1",
    "@types/mocha": "7.0.2",
    "@types/node": "13.9.5",
    "@types/sinon": "7.5.2",
    "@types/sinon-chai": "3.2.3",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/eslint-plugin-tslint": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "c21e": "2.0.0",
    "chai": "4.2.0",
    "chai-as-promised": "^7.1.1",
    "codelyzer": "5.2.2",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-chai-friendly": "0.5.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prefer-arrow": "1.1.7",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "gherkin": "9.0.0",
    "import": "0.0.6",
    "mocha": "7.1.1",
    "mocha-parallel-tests": "2.3.0",
    "mocha-sinon": "2.1.0",
    "npm-check-updates": "4.1.0",
    "nyc": "15.0.0",
    "prettier": "2.0.2",
    "rxjs-tslint": "0.1.7",
    "sinon": "9.0.1",
    "sinon-chai": "3.5.0",
    "ts-node": "8.8.1",
    "tslint": "6.1.0",
    "tslint-to-eslint-config": "0.5.1",
    "typescript": "3.8.3"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  },
