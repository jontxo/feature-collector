{
  "name": "feature-collector",
  "version": "1.0.3",
  "description": "collect feature files in an object",
  "keywords": [
    "cucumber",
    "features",
    "gherkin"
  ],
  "main": "lib/feature-collector.js",
  "scripts": {
    "test": "NODE_ENV=test mocha-parallel-tests -r ts-node/register --recursive test/**/*-spec.*  --timeout 50000 --slow 1000 ",
